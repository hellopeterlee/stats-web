.container
    .navbar-header
        button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
        a.navbar-brand(href='#') #{brand}
    .navbar-collapse.collapse
        ul.nav.navbar-nav
            - var obj = { 'home':'Home', 'about':'About' }
                - each val, key in obj
                    - if (id == key)
                        li.active
                            a(href='/') #{val}
                    - else
                        li
                            a(href='/') #{val}